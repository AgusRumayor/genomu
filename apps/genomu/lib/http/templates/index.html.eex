<body ng-app="genomuApp">
    <!-- start: Header -->
  <div class="navbar">
    <div class="navbar-inner">
      <div class="container-fluid">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
        <a class="brand" href="/"><span>Genomu</span>
          <span ng-controller="DashboardCtrl" ng-bind="instance.system_version"></span></a>
                
        <!-- start: Header Menu -->
        <div class="nav-no-collapse header-nav" ng-controller="DashboardCtrl">
          <ul class="nav pull-right">
            <li class="dropdown">
              <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                <i class="halflings-icon white hdd"></i><span ng-bind="instance.instance_url"></span>
                <span class="caret"></span>
              </a>
              <ul class="dropdown-menu" >
                <li ng-repeat="i in clusterMembership.instances"><a href="{{ i.url }}"><i class="halflings-icon white hdd"></i> {{ i.url }} </a></li>
              </ul>
            </li>
            <!-- end: User Dropdown -->
           </ul>
        </div>
        <!-- end: Header Menu -->
        
      </div>
    </div>
  </div>
  <!-- start: Header -->
  
    <div class="container-fluid">
    <div class="row-fluid">
        
    <div ng-view>
    </div>

    </div><!--/fluid-row-->
    
    <div class="clearfix"></div>
    <footer>
      <p>
        <span style="text-align:left;float:left">&copy; <a href="" target="_blank">Spawngrid, Inc.</a> 2013</span>
      </p>

    </footer>
        
